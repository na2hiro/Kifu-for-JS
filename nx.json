{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build:watch",
          "lint",
          "test",
          "typecheck"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/bundle"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "website": {
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ]
    },
    "ghpages": {
      "dependsOn": [
        "^build",
        "build"
      ]
    },
    "ci:cy": {
      "dependsOn": [
        "build:prodLocal"
      ]
    },
    "build:prodLocal": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default"
    ]
  }
}
