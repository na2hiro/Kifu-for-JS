{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-regenerator",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }]
  ]
}
