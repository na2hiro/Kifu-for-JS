{
    "name": "kifu-for-js",
    "version": "3.2.0",
    "description": "Shogi Kifu viewer for JavaScript",
    "main": "bundle/kifu-for-js.min.js",
    "types": "bundle/src/index.d.ts",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "clean": "rm -rf ./bundle",
        "dev": "webpack-dev-server --open example.html --mode development",
        "ci": "run-s lint ci:build ci:test",
        "ci:start-server": "http-server . -p 8081",
        "ci:build": "npm run build -- --env ci",
        "ci:test": "run-p --aggregate-output test ci:test:cypress",
        "ci:test:cypress": "start-server-and-test ci:start-server 8081 cypress:run:ci",
        "build": "NODE_ENV=production webpack --mode=production --env production",
        "build:ghpages": "webpack --env production --env ghpages",
        "build:analyze": "webpack --env production --env analyze",
        "build:watch": "webpack --watch",
        "lint": "tslint -p ./",
        "lint:fix": "prettier ./src/** --write && tslint -p ./ --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:run:ci": "cypress run --config-file cypress.ci.json",
        "ghpages": "rm -rf ./public && mkdir -p ./public/src && mkdir -p ./public/out & npm run build",
        "postghpages": "mv ./bundle/public-bookmarklet.min.js ./public/src/ && cp ./bundle/*.js ./public/out/ && cp -R examples ./public && cp -R bundle readme-ss public/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/na2hiro/Kifu-for-JS.git"
    },
    "keywords": [
        "shogi",
        "kifu",
        "viewer",
        "player",
        "kifuforjs",
        "jkf"
    ],
    "author": "na2hiro",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/na2hiro/Kifu-for-JS/issues"
    },
    "homepage": "https://github.com/na2hiro/Kifu-for-JS/tree/master/packages/Kifu-for-JS#readme",
    "dependencies": {
        "fscreen": "^1.1.0",
        "json-kifu-format": "^3.2.0",
        "mobx": "^4.1.1",
        "mobx-react": "^5.0.0",
        "react": "^16.0.0",
        "react-dnd": "^7.7.0",
        "react-dnd-html5-backend": "^2.1.2",
        "react-dnd-multi-backend": "^3.0.4",
        "react-dnd-touch-backend": "^0.8.3",
        "react-dom": "^16.0.0",
        "react-responsive": "^8.2.0",
        "shogi.js": "^3.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.3",
        "@babel/plugin-proposal-decorators": "^7.10.3",
        "@babel/polyfill": "^7.10.1",
        "@babel/preset-env": "^7.10.3",
        "@babel/preset-react": "^7.10.1",
        "@babel/preset-typescript": "^7.21.0",
        "@types/enzyme": "^3.1.10",
        "@types/react": "^16.1.0",
        "@types/react-dnd": "^2.0.36",
        "@types/react-dnd-html5-backend": "^2.1.9",
        "@types/react-dnd-multi-backend": "^3.0.1",
        "@types/react-dom": "^16.0.4",
        "babel-jest": "^29.5.0",
        "babel-loader": "^8.0.0-beta.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "clean-webpack-plugin": "^0.1.19",
        "core-js": "^3.6.5",
        "css-loader": "^3.6.0",
        "cypress": "^6.5.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.3",
        "http-server": "^14.1.1",
        "img-loader": "^4.0.0",
        "jest-environment-jsdom": "^28.1.2",
        "jquery": "^3.3.1",
        "mobx-react-devtools": "^5.0.1",
        "npm-run-all": "^4.1.5",
        "prop-types": "^15.6.1",
        "sass": "^1.30.0",
        "sass-loader": "^10.1.0",
        "start-server-and-test": "^1.12.0",
        "style-loader": "^0.20.3",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "tslint-react": "^3.5.1",
        "url-loader": "^4.1.1",
        "webpack-dev-server": "^4.9.3"
    }
}
