{
    "name": "kifu-for-js",
    "version": "5.1.0",
    "description": "Shogi Kifu viewer for JavaScript",
    "main": "dist/kifu-for-js.js",
    "types": "dist/src/index.d.ts",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "clean": "rm -rf ./bundle",
        "dev": "webpack-dev-server --open examples/example-v5.html --mode development",
        "ci:cy": "start-server-and-test ci:start-server :8081 cy:run:ci",
        "ci:start-server": "http-server . -p 8081",
        "build:prodLocal": "npm run build -- --env prodLocal",
        "build": "NODE_ENV=production webpack --mode=production --env production",
        "build:ghpages": "webpack --env production --env ghpages",
        "build:analyze": "webpack --env production --env analyze",
        "build:watch": "webpack --watch",
        "lint": "eslint ./ && prettier . --check",
        "lint:fix": "eslint ./ --fix && prettier . --write",
        "test": "jest",
        "test:watch": "jest --watch",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "cy:run:ci": "echo \"Running Cypress...\" && cypress run --config-file cypress.ci.config.ts",
        "ghpages": "rm -rf ./public && mkdir -p ./public/src && mkdir -p ./public/out & npm run build",
        "postghpages": "cp ./bundle/public-bookmarklet.min.js ./public/src/ && cp ./bundle/*.js ./public/out/ && cp -R examples ./public && cp -R bundle readme-ss public/",
        "typecheck": "tsc --project tsconfig.typecheck.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/na2hiro/Kifu-for-JS.git"
    },
    "keywords": [
        "shogi",
        "kifu",
        "viewer",
        "player",
        "kifuforjs",
        "jkf"
    ],
    "author": "na2hiro",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/na2hiro/Kifu-for-JS/issues"
    },
    "homepage": "https://github.com/na2hiro/Kifu-for-JS/tree/master/packages/Kifu-for-JS#readme",
    "dependencies": {
        "focus-trap-react": "^10.2.1",
        "json-kifu-format": "^5.1.0",
        "mobx": "^4.1.1",
        "mobx-react": "^6.3.1",
        "react-dnd": "^7.7.0",
        "react-dnd-html5-backend": "^2.1.2",
        "react-dnd-multi-backend": "^3.0.4",
        "react-dnd-touch-backend": "^0.8.3",
        "shogi.js": "^5.1.0",
        "use-long-press": "^3.2.0"
    },
    "peerDependencies": {
        "react": "^16.14.0",
        "react-dom": "^16.14.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.3",
        "@babel/plugin-proposal-decorators": "^7.10.3",
        "@babel/polyfill": "^7.10.1",
        "@babel/preset-env": "^7.10.3",
        "@babel/preset-react": "^7.10.1",
        "@babel/preset-typescript": "^7.21.0",
        "@testing-library/cypress": "^9.0.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@types/react": "^16.1.0",
        "@types/react-dnd": "^2.0.36",
        "@types/react-dnd-html5-backend": "^2.1.9",
        "@types/react-dnd-multi-backend": "^3.0.1",
        "@types/react-dom": "^16.0.4",
        "babel-jest": "^29.5.0",
        "babel-loader": "^8.0.0-beta.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "clean-webpack-plugin": "^0.1.19",
        "core-js": "^3.6.5",
        "css-loader": "^6.8.1",
        "cypress": "^12.10.0",
        "eslint": "^8.48.0",
        "http-server": "^14.1.1",
        "img-loader": "^4.0.0",
        "jest-environment-jsdom": "^28.1.2",
        "jquery": "^3.3.1",
        "npm-run-all": "^4.1.5",
        "prop-types": "^15.6.1",
        "sass": "^1.30.0",
        "sass-loader": "^10.1.0",
        "start-server-and-test": "^2.0.0",
        "style-loader": "^3.3.3",
        "url-loader": "^4.1.1",
        "webpack-dev-server": "^4.9.3"
    }
}
